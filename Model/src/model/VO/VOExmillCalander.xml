       EOExmillCalander.CARTON, 
       EOExmillCalander.CONTAINER_BOOKED, 
       EOExmillCalander.CONTAINER_BOOKING_DATE, 
       EOExmillCalander.CREATED_BY, 
       EOExmillCalander.CREATION_DATE, 
       EOExmillCalander.CSTK_PL, 
       EOExmillCalander.CURRENCY_CODE, 
       EOExmillCalander.CUST_PO_NO, 
       EOExmillCalander.DYEING_INPUT_DATE, 
       EOExmillCalander.DYEING_STATUS, 
       EOExmillCalander.DYEING_WO_NO, 
       EOExmillCalander.ELASTIC_CORD_FUSING, 
       EOExmillCalander.EX_MILL, 
       EOExmillCalander.GREIGE_COMPLETION_DATE, 
       EOExmillCalander.GREIGE_CONTRACT_NO, 
       EOExmillCalander.IMPORTED_MISC, 
       EOExmillCalander.INHOUSE_STICKERS, 
       EOExmillCalander.INSERTS, 
       EOExmillCalander.INSPECTION_DATE, 
       EOExmillCalander.INSPECTION_DONE, 
       EOExmillCalander.LAST_UPDATE_DATE, 
       EOExmillCalander.LAST_UPDATED_BY, 
       EOExmillCalander.LC_STATUS, 
       EOExmillCalander.MISC, 
       EOExmillCalander.NER, 
       EOExmillCalander.NORMAL_THREAD, 
       EOExmillCalander.PO_REC_DATE, 
       EOExmillCalander.PO_STATUS_LOOKUP_CODE, 
       EOExmillCalander.PO_TYPE, 
       EOExmillCalander.PROGRAM_ID, 
       EOExmillCalander.REMARKS, 
       EOExmillCalander.SALES_CONTRACT_NO, 
       EOExmillCalander.SHIP_MODE, 
       EOExmillCalander.SHIP_TERM, 
       EOExmillCalander.SHIPMENT_DATE, 
       EOExmillCalander.SP_HEADER_ID, 
       EOExmillCalander.STIFFERS, 
       EOExmillCalander.STITCH_INPUT_DATE, 
       EOExmillCalander.SYLKO_THREAD, 
       EOExmillCalander.TARGET_DATE, 
       EOExmillCalander.VALUE, 
       EOExmillCalander.VERSION_NUMBER, 
       EOExmillCalander.VIEWER_NOTE,
       ac.customer_name CUST_NAME,
       EOExmillCalander.THIRD_PARTY_AUDIT
FROM XX_SALES_PLANNING_HEADERS EOExmillCalander,
         XX_SALES_PLANNING_PROGRAM SPP,
         AR_CuSToMERS AC,
         fnd_user fu
WHERE EOExmillCalander.program_id = spp.program_id 
and     spp.cust_acct_id = ac.customer_id
and     SPP.program_manager = fu.user_id 
AND ( 
            TO_CHAR( EOExmillCalander.EX_MILL,'MON-YYYY') = to_char(trunc(:currDate),'MON-YYYY') OR
            TO_CHAR( EOExmillCalander.EX_MILL,'MON-YYYY') = to_char(trunc(ADD_MONTHS(:currDate,1)),'MON-YYYY')
       ) 
and  EOExmillCalander.EX_MILL is not null]]>
  </SQLQuery>
