  <SQLQuery>
    <![CDATA[SELECT spp.program_name,
fu.user_name,
EOExmillCalander.ACCESSORIES, 
       EOExmillCalander.ACCESSORIES_ORDER_DATE, 
       EOExmillCalander.BAGS, 
       EOExmillCalander.BOOK_MONTH, 
       EOExmillCalander.BOOKING_MONTH, 
